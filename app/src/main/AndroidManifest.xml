<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zoomsoft">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZoomSoft">
        <activity android:name=".eventInfo.HabitInfo"
            android:exported="true"/>
        <activity
            android:name=".MainPageTabs"
            android:exported="true"
            android:label="@string/title_activity_main_page_tabs"
            android:theme="@style/Theme.ZoomSoft.NoActionBar" />
        <activity
            android:name=".loginandregister.login"
            android:exported="true" />
        <activity
            android:name=".loginandregister.register"
            android:exported="true" />
        <activity
            android:name=".HabitInfo"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>